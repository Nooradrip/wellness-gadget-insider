{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "src/app/sitemaps/route.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/sitemaps/sitemap.xml",
      "dest": "/src/app/sitemaps/route.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "build": {
    "env": {
      "NEXT_PRIVATE_TARGET": "server",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "installCommand": "npm install --prefer-offline --legacy-peer-deps",
  "framework": "nextjs"
}